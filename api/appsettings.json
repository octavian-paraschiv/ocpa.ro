{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "%LOGDIR%/ocpa_ro_api_.log",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 2
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "Authentication": {
    "Jwt": {
      "Issuer": "ocpa.ro",
      "Audience": "ocpa.ro",
      "Validity": 900
    },
    "UseOTP": false,
    "OTPDuration": 180,
    "PasswordHistoryLength": 5,
    "MaxPasswordAge": 90,
    "MaxLoginRetries": 5
  },
  "Email": {
    "ServerAddress": "mail.2mwin-dns.com",
    "ServerPort": 25,
    "FromAddress": "do_not_reply@ocpa.ro",
    "FromName": "OCPA.RO EMAIL SENDER",
    "Credentials": "NU1XTUpKVGUwWTNJID04bWN1RUdjajlHUTV4R2NsSjNYMDltYmY5R1o="
  },
  "Geolocation": {
    "BaseUrl": "http://ip-api.com/json/"
  },
  "CaaS": {
    "BaseUrl": "https://cataas.com/",
    "CachePeriod": 900
  }
}
